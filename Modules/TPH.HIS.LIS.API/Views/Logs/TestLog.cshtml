@using TPH.HIS.WebAPI.Configs
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="page-header">
    <div class="input-group col-xs-8 col-sm-5">
        Test - Insert Logs
    </div>
</div>

<!-- /.page-header -->
<div class="row">
    <div class="col-sm-8">
        <div class="widget-box widget-color-green">
            <div class="widget-header widget-header-small">  </div>

            <div class="widget-body">
                <div class="widget-main no-padding">
                    <textarea id="message" placeholder="Enter Message Log" class="col-sm-12" style="height:300px"></textarea>
                </div>
                

                <div class="widget-toolbox padding-4 clearfix">
                    <div class="btn-group pull-left">
                        <button class="btn btn-sm btn-default btn-white btn-round" id="btnCancel">
                            <i class="ace-icon fa fa-times bigger-125"></i>
                            Hủy
                        </button>
                        &nbsp;
                        <button class="btn btn-sm btn-danger btn-white btn-round" id="btnSave">
                            <i class="ace-icon fa fa-floppy-o bigger-125"></i>
                            Lưu
                        </button>
                    </div>

                    <div class="btn-group pull-left">

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


@section scripts{
    
     <script src="@Url.Content("~/Assets/ViewLogs/TestLog.js?ver="+ @HisApiCommonConfigs.JsVersion)" type="text/javascript"></script>
}
